{
  "tabWidth": 2,
  "useTabs": false,
  "parser": "typescript",
  "printWidth": 120,
  "arrowParens": "always",
  "bracketSpacing": true,
  "eslintIntegration": true,
  "ignorePath": ".prettierignore",
  "jsxBracketSameLine": false,
  "proseWrap": "preserve",
  "semi": false,
  "singleQuote": true,
  "stylelintIntegration": false,
  "trailingComma": "all",
  "jsxSingleQuote": true
}
